go 2
for A = 1 to NUM_GEN

	GENEROS[A] = GENERO->DADOS
	GEN_COR[A] = _left( GENERO->DADOS,1 )
	skip

next
close "GENERO"
NUM_LIN = iif( NUM_GEN > 15, 15 , NUM_GEN )
