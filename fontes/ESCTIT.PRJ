	popup	window	13,03,19,20;
			to			OP;
			options	QUEM;
			hilite 	Q_LL,"GR+/R";
			color 	"W+/R,N/W,N,W+/R"
	if 	OP = 1

		if lastkey() # 27

			* NOVAS_FITAS

		endif

	elseif OP = 2

		if lastkey() # 27

			Marca_titulo()

		endif

	elseif OP = 3

		if lastkey() # 27

			Genero()

		endif

	elseif OP = 4

		Quantas()

		if lastkey() # 27

			Imprime_etiqueta_fita()		

		endif

	elseif OP = 5

		Tira_marca()

	elseif OP = 6

		Definir()

	else

		exit

	endif

enddo

