Q_LL[2] = "F"
Q_LL[3] = "G"
Q_LL[4] = "I"
Q_LL[5] = "T"
Q_LL[6] = "E"
Q_LL[7] = "R"

do while .T.

	clear typeahead
	set color to "W+/B,N/W,N,W+/B"
