***************************************** && linha titulo original ************

?? chr( 27 ) + "3" + chr( 30 )
?  chr( 15 ) + MTORIG[1] +	space( 10 )
for LB10 = 2 to N_COLUNAS

	?? chr( 18 ) + space( P_ESP + 1 ) + chr( 15 ) +; && desativa caracter comprimido
		MTORIG[LB10] + space( 10 )                    && titulo original da fita

next

